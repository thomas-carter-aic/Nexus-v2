#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/oss/Repos/AIC/ai-platform/aic__ai-platform/node_modules/.pnpm/react-native@0.79.6_@babel+core@7.28.4_@react-native-community+cli@18.0.0_typescript@5._a7e868cb5a5045f05f27f7d2961dbc61/node_modules/react-native/node_modules:/home/oss/Repos/AIC/ai-platform/aic__ai-platform/node_modules/.pnpm/react-native@0.79.6_@babel+core@7.28.4_@react-native-community+cli@18.0.0_typescript@5._a7e868cb5a5045f05f27f7d2961dbc61/node_modules:/home/oss/Repos/AIC/ai-platform/aic__ai-platform/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/oss/Repos/AIC/ai-platform/aic__ai-platform/node_modules/.pnpm/react-native@0.79.6_@babel+core@7.28.4_@react-native-community+cli@18.0.0_typescript@5._a7e868cb5a5045f05f27f7d2961dbc61/node_modules/react-native/node_modules:/home/oss/Repos/AIC/ai-platform/aic__ai-platform/node_modules/.pnpm/react-native@0.79.6_@babel+core@7.28.4_@react-native-community+cli@18.0.0_typescript@5._a7e868cb5a5045f05f27f7d2961dbc61/node_modules:/home/oss/Repos/AIC/ai-platform/aic__ai-platform/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-native/cli.js" "$@"
else
  exec node  "$basedir/../react-native/cli.js" "$@"
fi
